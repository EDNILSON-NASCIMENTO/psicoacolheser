generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("MYSQL_DATABASE_URL")
}

model users {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  user_type UserType
}

enum UserType {
  PSICOLOGO
  PACIENTE
  ADMIN
}
