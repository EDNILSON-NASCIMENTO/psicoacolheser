{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "outputs": [".next/**", "dist/**"]
    },    
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^prisma:generate"]
    },
    "prisma:generate": {
      "outputs": ["node_modules/.prisma"],
      "cache": false
    }
  }
}